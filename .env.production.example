# =====================================================================
# PRODUCTION ENVIRONMENT VARIABLES
# Copy this file to .env and fill in your actual values
# NEVER commit the actual .env file to version control
# =====================================================================

# SUPABASE CONFIGURATION (REQUIRED)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# ZOOM VIDEO SDK (REQUIRED FOR VIDEO CALLS)
# SECURITY WARNING: These should be handled server-side in production
VITE_ZOOM_API_KEY=your_zoom_api_key
VITE_ZOOM_API_SECRET=your_zoom_api_secret

# AI SERVICES (OPTIONAL - for CV processing)
VITE_GEMINI_API_KEY=your_gemini_api_key

# EMAIL SERVICE (OPTIONAL)
VITE_EMAIL_API_KEY=your_email_service_api_key
VITE_EMAIL_FROM=noreply@yourdomain.com

# PUSH NOTIFICATIONS (OPTIONAL)
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key
VITE_VAPID_PRIVATE_KEY=your_vapid_private_key

# APPLICATION SETTINGS
VITE_APP_NAME="GradApp Ascend"
VITE_APP_URL=https://yourdomain.com
VITE_API_URL=https://api.yourdomain.com

# FEATURE FLAGS
VITE_ENABLE_AI_FEATURES=false
VITE_ENABLE_VIDEO_CALLS=true
VITE_ENABLE_PUSH_NOTIFICATIONS=false

# MONITORING (OPTIONAL)
VITE_SENTRY_DSN=your_sentry_dsn
VITE_GA_TRACKING_ID=your_google_analytics_id

# DEVELOPMENT/STAGING/PRODUCTION
NODE_ENV=production